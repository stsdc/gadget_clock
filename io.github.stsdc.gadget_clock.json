{
    "id" : "io.github.stsdc.gadget_clock",
    "runtime" : "io.elementary.Platform",
    "runtime-version" : "8.2",
    "sdk" : "io.elementary.Sdk",
    "command" : "io.github.stsdc.gadget_clock",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri"
        ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
    "gtkmm.json",
        {
            "name" : "gadget_clock",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "path": ".",
                    "type": "dir"
                }
            ],
            "config-opts" : [
                "--libdir=lib"
            ]
        }
    ],
    "build-options" : {
        "env" : {        }
    }
}
